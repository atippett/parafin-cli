{
  "name": "parafin-cli",
  "version": "1.0.0",
  "description": "CLI tool for interacting with the Parafin API",
  "main": "dist/index.js",
  "bin": {
    "parafin": "./bin/parafin-standalone.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "parafin",
    "cli",
    "api",
    "finance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^3.0.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.1",
    "json-colorizer": "^3.0.1",
    "puppeteer": "^21.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "parafin",
    "dirname": "parafin-cli"
  }
}
